{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "defaultProject": "ondewo-proto-client-angular",
  "projects": {
    "ondewo-proto-client-angular": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "./src/tsconfig.json"
            }
          },
          "options": {
            "project": "./src",
            "tsConfig": "./src/tsconfig.json"
          }
        }
      },
      "prefix": "lib",
      "projectType": "library",
      "root": "./src",
      "sourceRoot": "./src"
    }
  },
  "version": "5.4.0"
}
